            "warn": false,
            "allow_missing_keys": true
        },
        {
            "_target_": "RandZoomd",
            "keys": [
                "@image_key",
                "@label_key"
            ],
            "min_zoom": 0.8,
            "max_zoom": 1.2,
            "mode": [
                "trilinear",
                "nearest"
            ],
            "prob": 0.2,
            "allow_missing_keys": true
        },
        {
            "_target_": "RandSimulateLowResolutiond",
            "keys": [
                "@image_key"
            ],
            "zoom_range": [
                0.3,
                1
            ],
            "prob": 0.2,
            "allow_missing_keys": true
        },
        {
            "_target_": "RandGaussianSmoothd",
            "keys": [
                "@image_key"
            ],
            "prob": 0.2,
            "sigma_x": [
                0.5,
                1.0
            ],
            "sigma_y": [
                0.5,
                1.0
            ],
            "sigma_z": [
                0.5,
                1.0
            ]
        },
        {
            "_target_": "RandScaleIntensityd",
            "keys": [
                "@image_key"
            ],
            "factors": 0.1,
            "prob": 0.2
        },
        {
            "_target_": "RandShiftIntensityd",
            "keys": [
                "@image_key"
            ],
            "offsets": 0.1,
            "prob": 0.2
        },
        {
            "_target_": "RandGaussianNoised",
            "keys": [
                "@image_key"
            ],
            "prob": 0.2,
            "mean": 0.0,
            "std": 0.2
        }
    ],
